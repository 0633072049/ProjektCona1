<script>
    function myMap() {
        var mapCanvas = document.getElementById("map");
        var mapOptions = {
            center: new google.maps.LatLng(45.952766, 13.650980), zoom: 14,
            panControl: false,
            zoomControl: false,
            mapTypeControl: true,
            scaleControl: false,
            streetViewControl: false,
            overviewMapControl: false,
            rotateControl: false,
            gestureHandling: 'cooperative'
        };
        var map = new google.maps.Map(mapCanvas, mapOptions);

        var marker1 = new google.maps.Marker({
            position: new google.maps.LatLng(45.952720, 13.668899),
            label: "1"

        });
        var marker2 = new google.maps.Marker({
            position: new google.maps.LatLng(45.949988, 13.650484),
            label: "2"

        });
        var marker3 = new google.maps.Marker({
            position: new google.maps.LatLng(45.953065, 13.636372),
            label: "3"

        });

        marker1.setMap(map);
        marker2.setMap(map);
        marker3.setMap(map);

        marker1.addListener('click', function () { location.href = "/Podatki/Index/"; });
        marker2.addListener('click', function () { location.href = "/Podatki/Index/"; });
        marker3.addListener('click', function () { location.href = "/Podatki/Index/"; });
    }

</script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDYVCxh0G-I-FtUD6b1hMUWiQ6pY4XSpdk&callback=myMap"></script>